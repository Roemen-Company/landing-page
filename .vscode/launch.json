    "version": "0.2.0",
    "configurations": [
        {
            "type": "blazorwasm",
            "request": "attach",
            "name": "Attach and Debug",
            "url": "http://localhost:5000",
            "browser": "edge"
        },
        {
            "name": "Launch and Debug Hosted Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "hosted": true,
            "program": "${workspaceFolder}/src/Roemen.Company.Web/bin/Debug/netstandard2.1/Roemen.Company.Web.dll",
            "cwd": "${workspaceFolder}/src/Roemen.Company.Web/",
            "browser": "edge"
        }
    ]
}